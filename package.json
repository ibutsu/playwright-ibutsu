{
  "name": "playwright-ibutsu",
  "version": "0.0.0-development",
  "description": "A Playwright test reporter for uploading results to Ibutsu",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "precommit": "npm run lint:fix && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibutsu/playwright-ibutsu.git"
  },
  "keywords": [
    "playwright",
    "reporter",
    "ibutsu",
    "testing",
    "test-results"
  ],
  "author": "Mike Shriver",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "ibutsu-client-ts": "^2.0.4",
    "@playwright/test": "^1.48.0",
    "tar": "^7.4.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.0",
    "@types/tar": "^6.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "peerDependencies": {
    "@playwright/test": ">=1.40.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "arrowParens": "always",
    "endOfLine": "lf",
    "proseWrap": "preserve"
  }
}
